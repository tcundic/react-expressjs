{
  "name": "react-expressjs",
  "version": "1.0.0",
  "description": "Example application of React with ExpressJS",
  "main": "index.js",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  },
  "scripts": {
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "serve": "lite-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tcundic/react-expressjs.git"
  },
  "author": "Tomislav CundiÄ‡",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tcundic/react-expressjs/issues"
  },
  "homepage": "https://github.com/tcundic/react-expressjs#readme",
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@mdi/font": "^5.8.55",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.5.3",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "express": "^4.17.1",
    "html-webpack-plugin": "^4.5.0",
